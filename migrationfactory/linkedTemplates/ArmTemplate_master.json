{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "migrationfactory"
		},
		"LS_ADLSGen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLSGen2'"
		},
		"LS_madhuadlsgen1_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'LS_madhuadlsgen1'"
		},
		"LS_ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sacadlsgen2.dfs.core.windows.net"
		},
		"LS_madhuadlsgen1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://madhuadlsweu.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_madhuadlsgen1_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "17125979-8f9e-427f-a2b7-f658c8dca12d"
		},
		"LS_madhuadlsgen1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "52bd225b-21f6-4110-b5e6-1752567d3f8e"
		},
		"LS_madhuadlsgen1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "5a146b9c-0e6e-44db-9bd4-b804364f80b4"
		},
		"LS_madhuadlsgen1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "adls"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLSGen2_accountKey": {
						"value": "[parameters('LS_ADLSGen2_accountKey')]"
					},
					"LS_madhuadlsgen1_servicePrincipalKey": {
						"value": "[parameters('LS_madhuadlsgen1_servicePrincipalKey')]"
					},
					"LS_ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLSGen2_properties_typeProperties_url')]"
					},
					"LS_madhuadlsgen1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('LS_madhuadlsgen1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"LS_madhuadlsgen1_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('LS_madhuadlsgen1_properties_typeProperties_servicePrincipalId')]"
					},
					"LS_madhuadlsgen1_properties_typeProperties_tenant": {
						"value": "[parameters('LS_madhuadlsgen1_properties_typeProperties_tenant')]"
					},
					"LS_madhuadlsgen1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('LS_madhuadlsgen1_properties_typeProperties_subscriptionId')]"
					},
					"LS_madhuadlsgen1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('LS_madhuadlsgen1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}