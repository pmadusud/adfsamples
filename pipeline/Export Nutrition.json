{
	"name": "Export Nutrition",
	"properties": {
		"activities": [
			{
				"name": "Export Nutrition Data",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "CosmosDbSqlApiSource",
						"preferredRegions": []
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings",
							"copyBehavior": "FlattenHierarchy"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['id']"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"path": "$['_rid']"
								},
								"sink": {
									"name": "_rid"
								}
							},
							{
								"source": {
									"path": "$['_self']"
								},
								"sink": {
									"name": "_self"
								}
							},
							{
								"source": {
									"path": "$['_etag']"
								},
								"sink": {
									"name": "_etag"
								}
							},
							{
								"source": {
									"path": "$['description']"
								},
								"sink": {
									"name": "description"
								}
							},
							{
								"source": {
									"path": "$['tags'][0]['name']"
								},
								"sink": {
									"name": "name"
								}
							},
							{
								"source": {
									"path": "$['version']"
								},
								"sink": {
									"name": "version"
								}
							},
							{
								"source": {
									"path": "$['foodGroup']"
								},
								"sink": {
									"name": "foodgroup"
								}
							},
							{
								"source": {
									"path": "$['nutrients'][0]['id']"
								},
								"sink": {
									"name": "nutrients-id"
								}
							},
							{
								"source": {
									"path": "$['nutrients'][0]['description']"
								},
								"sink": {
									"name": "nutrients-description"
								}
							},
							{
								"source": {
									"path": "$['nutrients'][0]['nutritionValue']"
								},
								"sink": {
									"name": "nutrients-nutritionvalue"
								}
							},
							{
								"source": {
									"path": "$['nutrients'][0]['units']"
								},
								"sink": {
									"name": "nutrients-units"
								}
							},
							{
								"source": {
									"path": "['amount']"
								},
								"sink": {
									"name": "servings-amount"
								}
							},
							{
								"source": {
									"path": "['description']"
								},
								"sink": {
									"name": "servings-description"
								}
							},
							{
								"source": {
									"path": "['weightInGrams']"
								},
								"sink": {
									"name": "servings-weightingrams"
								}
							},
							{
								"source": {
									"path": "$['_attachments']"
								},
								"sink": {
									"name": "_attachments"
								}
							},
							{
								"source": {
									"path": "$['_ts']"
								},
								"sink": {
									"name": "_ts"
								}
							},
							{
								"source": {
									"path": "$['isFromSurvey']"
								},
								"sink": {
									"type": "String"
								}
							}
						],
						"collectionReference": "$['servings']"
					}
				},
				"inputs": [
					{
						"referenceName": "DS_NutritionDataCosmosDb",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "NutritionExtract",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}